# Environment Configuration for Sparkrock API
# Copy this file to .env and modify values as needed

# Server Configuration
PORT=3000
NODE_ENV=staging

# CORS Configuration
CORS_ORIGIN=*

# Logging
LOG_LEVEL=info

# API Configuration
API_VERSION=v1
API_TIMEOUT=30000

# Security
RATE_LIMIT_WINDOW=15
RATE_LIMIT_MAX_REQUESTS=100
